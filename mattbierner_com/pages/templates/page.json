{
    {% load json_filter %}
    
    'title': {{ self.title|as_json }},
    'tags': [
        {% for tag in self.tags.all %}
            {{ tag.name|as_json}},
        {% endfor %}
    ],
    'brief': {{ self.brief|as_json }},
    'header': {{ self.header.rendered|as_json }},
    'body': {{ self.body.rendered|as_json }}
}